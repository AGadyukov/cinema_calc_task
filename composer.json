{
  "require": {
    "php": ">=7.4",
    "psr/log": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "phpstan/phpstan": "^0.12.88"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests/"
    }
  },
  "scripts": {
    "linters": [
      "vendor/bin/phpstan analyse src/ --level max"
    ]
  }
}
